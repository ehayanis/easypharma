<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="easypharma" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>com.mm.app.model.Address</class>
		<class>com.mm.app.model.Assurance</class>
		<class>com.mm.app.model.Client</class>
		<class>com.mm.app.model.Medecin</class>
		<class>com.mm.app.model.Ordonnance</class>
		<class>com.mm.app.model.Product</class>
<!-- 		<exclude-unlisted-classes>false</exclude-unlisted-classes> -->
		
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/eashpharma" />
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
			<property name="javax.persistence.jdbc.user" value="easypharma" />
			<property name="javax.persistence.jdbc.password" value="admin1234" />
			<property name="eclipselink.logging.level" value="ALL" />
		</properties>
	</persistence-unit>
</persistence>
